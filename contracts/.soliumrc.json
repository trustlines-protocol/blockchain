{
  "extends": "solium:all",
  "plugins": [
    "security"
  ],
  "rules": {
    "indentation": [
      "warning",
      4
    ],
    "function-order": "error",
    "mixedcase": "error",
    "quotes": [
      "error",
      "double"
    ],
    "security/no-block-members": "off",
    "blank-lines": "off",
    "lbrace": "off"
  }
}
